<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.geno.payment"
    android:versionCode="4"
    android:versionName="1.3" >

    <uses-sdk
        android:minSdkVersion="23"
        android:targetSdkVersion="8" />

    <uses-permission android:name="android.permission.READ_CALENDAR" />
    <uses-permission android:name="android.permission.INTERNET" />
    <uses-permission android:name="android.permission.ACCESS_NETWORK_STATE" />
    <uses-permission android:name="android.permission.WAKE_LOCK" />
    <uses-permission android:name="android.permission.NFC" />
    <uses-permission android:name="android.permission.READ_PHONE_STATE" />
    <uses-permission android:name="android.permission.WRITE_EXTERNAL_STORAGE" />
    <uses-permission android:name="android.permission.ACCESS_FINE_LOCATION" />
    <uses-permission android:name="android.permission.USE_FINGERPRINT" />

    <uses-feature
        android:name="android.hardware.nfc"
        android:required="true" />

    <uses-permission android:name="android.permission.WRITE_EXTERNAL_STORAGE" />
    <uses-permission android:name="android.permission.READ_EXTERNAL_STORAGE" />
    <uses-permission android:name="android.permission.VIBRATE" />

    <meta-data
        android:name="android.support.VERSION"
        android:value="25.3.1" />

    <uses-permission android:name="com.google.android.c2dm.permission.RECEIVE" />

    <permission
        android:name="com.geno.payment.permission.C2D_MESSAGE"
        android:protectionLevel="signature" />

    <uses-permission android:name="com.geno.payment.permission.C2D_MESSAGE" />

    <application
        android:allowBackup="true"
        android:icon="@drawable/logo_ttt"
        android:label="2 factor 전자결제시스템"
        android:theme="@style/AppTheme" >
        <activity
            android:name="com.geno.MainActivity"
            android:label="@string/app_name2" />
        <activity
            android:name="com.ajantech.nfcpaymentsystem.ui.Start"
            android:label="@string/app_name2"
            android:theme="@android:style/Theme.DeviceDefault.Light" />
        <activity
            android:name="com.ajantech.nfcpaymentsystem.ui.Title"
            android:label="@string/app_name"
            android:screenOrientation="portrait" />
        <activity
            android:name="com.ajantech.nfcpaymentsystem.ui.GeneralConditions"
            android:configChanges="orientation|keyboardHidden"
            android:label="@string/app_name"
            android:screenOrientation="portrait" />
        <activity
            android:name="com.ajantech.nfcpaymentsystem.ui.Main"
            android:configChanges="orientation|keyboardHidden"
            android:label="@string/app_name"
            android:screenOrientation="portrait" >
            <intent-filter>
                <action android:name="android.nfc.action.NDEF_DISCOVERED" />

                <category android:name="android.intent.category.DEFAULT" />

                <data android:mimeType="text/plane" />
            </intent-filter>
        </activity>
        <activity
            android:name="com.ajantech.nfcpaymentsystem.ui.PwKeypad"
            android:configChanges="orientation|keyboardHidden"
            android:label="@string/app_name"
            android:screenOrientation="portrait" />
        <activity
            android:name="com.ajantech.nfcpaymentsystem.ui.PwCommon"
            android:configChanges="orientation|keyboardHidden"
            android:screenOrientation="portrait" />
        <activity
            android:name="com.ajantech.nfcpaymentsystem.ui.AddRegister"
            android:configChanges="orientation|keyboardHidden"
            android:label="@string/app_name"
            android:screenOrientation="portrait" />
        <activity
            android:name="com.ajantech.nfcpaymentsystem.ui.ModifyInformation"
            android:configChanges="orientation|keyboardHidden"
            android:label="@string/app_name"
            android:screenOrientation="portrait" />
        <activity
            android:name="com.ajantech.nfcpaymentsystem.ui.NFCPaymentClient"
            android:label="@string/app_name"
            android:screenOrientation="portrait" >
            <intent-filter>
                <action android:name="android.nfc.action.NDEF_DISCOVERED" />

                <category android:name="android.intent.category.DEFAULT" />

                <data android:mimeType="text/plane" />
            </intent-filter>
        </activity>
        <activity
            android:name="com.ajantech.nfcpaymentsystem.ui.NFCPaymentRetailer"
            android:label="@string/app_name"
            android:screenOrientation="portrait" >
            <intent-filter>
                <action android:name="android.nfc.action.NDEF_DISCOVERED" />

                <category android:name="android.intent.category.DEFAULT" />

                <data android:mimeType="text/plane" />
            </intent-filter>
        </activity>
        <activity
            android:name="com.ajantech.nfcpaymentsystem.ui.RetailSearchMain"
            android:label="@string/app_name"
            android:screenOrientation="portrait" />
        <activity
            android:name="com.ajantech.nfcpaymentsystem.ui.RetailSearchDate"
            android:label="@string/app_name"
            android:screenOrientation="portrait" />
        <activity
            android:name="com.ajantech.nfcpaymentsystem.ui.RetailSearchProduct"
            android:label="@string/app_name"
            android:screenOrientation="portrait" />

        <service
            android:name="com.ajantech.nfc_network.service.NetworkService"
            android:exported="false" >
            <intent-filter>
                <action android:name="com.ajantech.nfc_network.heh" />
            </intent-filter>
        </service>

        <activity
            android:name="com.ajantech.nfcpaymentsystem.LodingActivity"
            android:theme="@android:style/Theme.NoTitleBar.Fullscreen" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name="com.geno.coupon_list"
            android:label="@string/app_name2" >
            <intent-filter>
                <action android:name="android.nfc.action.NDEF_DISCOVERED" />

                <category android:name="android.intent.category.DEFAULT" />

                <data android:mimeType="application/com.geno.payment" />
            </intent-filter>
        </activity>
        <activity
            android:name="com.geno.IssueCoupon"
            android:label="@string/app_name" />
        <activity
            android:name="com.geno.CouponDetailCal"
            android:label="@string/app_name" />
        <activity
            android:name="com.geno.CouponDetailPaid"
            android:label="@string/app_name" />
        <activity
            android:name="com.geno.Agreement"
            android:label="@string/app_name" />
        <activity
            android:name="com.geno.Join"
            android:label="@string/app_name" />
        <activity
            android:name="com.geno.DbConnect"
            android:label="@string/app_name" />
        <activity android:name="com.GalleryAuction.UI.ArtInformation" />
        <activity android:name="com.GalleryAuction.UI.BidderInfo" />
        <activity android:name="com.GalleryAuction.UI.ArtistAuctionCompleteUi" />
        <activity
            android:name="com.GalleryAuction.UI.IamPortWebViewBidding"
            android:launchMode="singleTask" >
            <intent-filter>
                <action android:name="android.intent.action.VIEW" />

                <category android:name="android.intent.category.DEFAULT" />
                <category android:name="android.intent.category.BROWSABLE" />

                <data android:scheme="iamportkakao" />
            </intent-filter>
        </activity>
        <activity android:name="com.GalleryAuction.UI.ArtistMainActivity" />
        <activity
            android:name="com.GalleryAuction.Dialog.GridCustomDialog"
            android:theme="@android:style/Theme.Dialog" />
        <activity android:name="com.GalleryAuction.UI.BidderMainActivity" />
        <activity
            android:name="com.GalleryAuction.Dialog.TagExplanationDialog"
            android:theme="@android:style/Theme.Dialog" />
        <activity android:name="com.GalleryAuction.UI.BidderAddress" />
        <activity
            android:name="com.GalleryAuction.Dialog.WebViewAddressDialog"
            android:theme="@android:style/Theme.Holo.Light.Dialog" />
        <activity android:name="com.GalleryAuction.UI.ArtistAddress" />

        <service
            android:name="com.GalleryAuction.Service.MyService"
            android:enabled="true" />

        <activity
            android:name="com.GalleryAuction.Dialog.ChangePriceDialog"
            android:theme="@android:style/Theme.Holo.Light.Dialog" />
        <activity
            android:name="com.GalleryAuction.Dialog.ContractDialog"
            android:theme="@android:style/Theme.Dialog" >
        </activity>

        <receiver
            android:name="com.google.android.gms.measurement.AppMeasurementReceiver"
            android:enabled="true" >
            <intent-filter>
                <action android:name="com.google.android.gms.measurement.UPLOAD" />
            </intent-filter>
        </receiver>

        <service
            android:name="com.google.android.gms.measurement.AppMeasurementService"
            android:enabled="true"
            android:exported="false" />

        <activity
            android:name="com.google.android.gms.common.api.GoogleApiActivity"
            android:exported="false"
            android:theme="@android:style/Theme.Translucent.NoTitleBar" />
        <!--
 FirebaseMessagingService performs security checks at runtime,
             no need for explicit permissions despite exported="true"
        -->
        <service
            android:name="com.google.firebase.messaging.FirebaseMessagingService"
            android:exported="true" >
            <intent-filter android:priority="-500" >
                <action android:name="com.google.firebase.MESSAGING_EVENT" />
            </intent-filter>
        </service>

        <receiver
            android:name="com.google.firebase.iid.FirebaseInstanceIdReceiver"
            android:exported="true"
            android:permission="com.google.android.c2dm.permission.SEND" >
            <intent-filter>
                <action android:name="com.google.android.c2dm.intent.RECEIVE" />
                <action android:name="com.google.android.c2dm.intent.REGISTRATION" />

                <category android:name="com.geno.payment" />
            </intent-filter>
        </receiver>
        <!--
 Internal (not exported) receiver used by the app to start its own exported services
             without risk of being spoofed.
        -->
        <receiver
            android:name="com.google.firebase.iid.FirebaseInstanceIdInternalReceiver"
            android:exported="false" />
        <!--
 FirebaseInstanceIdService performs security checks at runtime,
             no need for explicit permissions despite exported="true"
        -->
        <service
            android:name="com.google.firebase.iid.FirebaseInstanceIdService"
            android:exported="true" >
            <intent-filter android:priority="-500" >
                <action android:name="com.google.firebase.INSTANCE_ID_EVENT" />
            </intent-filter>
        </service>

        <provider
            android:name="com.google.firebase.provider.FirebaseInitProvider"
            android:authorities="com.geno.payment.firebaseinitprovider"
            android:exported="false"
            android:initOrder="100" />

        <meta-data
            android:name="com.google.android.gms.version"
            android:value="@integer/google_play_services_version" />
    </application>

</manifest>