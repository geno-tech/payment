<?xml version="1.0" encoding="utf-8"?>
<layer-list xmlns:android="http://schemas.android.com/apk/res/android" >

    <item
        android:bottom="0dp"
        android:left="0dp"
        android:right="0dp"
        android:top="0dp">
        <shape>
            <gradient
                android:angle="270"
                android:endColor="#FF957f5b"
                android:startColor="#FF8e7a56" />

            <stroke
                android:width="0dp"
                android:color="#517a24" />

            <corners
                android:bottomLeftRadius="0dip"
                android:bottomRightRadius="0dip"
                android:topLeftRadius="0dip"
                android:topRightRadius="0dip" />
        </shape>
    </item>

</layer-list>